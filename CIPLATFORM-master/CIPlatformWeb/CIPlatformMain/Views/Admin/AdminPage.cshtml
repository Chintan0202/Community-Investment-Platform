<div class="d-md-flex d-lg-flex">


    <nav class="navbar navbar-expand-lg navbar-expand-md navbar-dark" style="background-color:  #F88634;">
        <div class="container-fluid">
            <p class="navbar-brand fw-bold" href="#">Admin</p>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse ms-0" id="navbarSupportedContent">
                <div class="d-flex align-items-start">
                    <div class="nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">
                        <button class="nav-link text-left my-2 active-tab" data-bs-toggle="pill" type="button" role="tab" id="UserList" onclick="GetUserData()"><i class="bi bi-person"></i>&nbsp; User</button>
                        <button class="nav-link text-left my-2" id="v-pills-CMS-tab" onclick="GetCMSList()" data-bs-toggle="pill" type="button" role="tab"><i class="bi bi-file-earmark-text"></i>&nbsp; CMS Page</button>
                        <button class="nav-link text-left my-2" id="v-pills-Mission-tab" onclick="GetMissionList()" data-bs-toggle="pill" type="button" role="tab"><i class="bi bi-bullseye"></i>&nbsp; Mission</button>
                        <button class="nav-link text-left my-2"  data-bs-toggle="pill" type="button" role="tab"  onclick="GetThemeList()"><i class="bi bi-columns-gap"></i>&nbsp; Mission Theme</button>
                        <button class="nav-link text-left my-2" id="v-pills-Missionskill-tab" onclick="GetSkillsList()" data-bs-toggle="pill"  type="button" role="tab"><i class="bi bi-tools"></i>&nbsp; Mission Skills</button>
                        <button class="nav-link text-left my-2" id="MissionApplicationList" data-bs-toggle="pill" type="button" role="tab" onclick="GetMissionApplication()"><i class="bi bi-folder-fill"></i>&nbsp; Mission Application</button>
                        <button class="nav-link text-left my-2" onclick="GetStoryList()" data-bs-toggle="pill" type="button" role="tab"><i class="bi bi-bookmark-star-fill"></i>&nbsp; Story</button>
                        <button class="nav-link text-left my-2" id="v-pills-Banner-tab" onclick="GetBannerList()" data-bs-toggle="pill" type="button" role="tab"><i class="bi bi-emoji-sunglasses"></i>&nbsp; Banner Management</button>
                    </div>

                </div>
            </div>
        </div>
    </nav>
    <div style="width:100%;">

        <div class="p-3 d-flex shadow" id="navbar-root">
            <span class="me-auto h4" id="clock"></span>
            <div class="d-flex">
                <img src='~/images/user-img-large.png' class="user-icon " />
                <div class="dropdown ms-2">
                    <button class="btn dropdown-toggle" type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false">
                        John Due
                    </button>
                    <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1">
