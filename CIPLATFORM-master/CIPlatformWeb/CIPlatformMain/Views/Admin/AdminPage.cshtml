@{
if (TempData["EmailStatus"]!=null)
{
     <script>
                        Swal.fire({
              position: 'top-end',
              icon: 'warning',
              title: 'Email Already Exists',
              showConfirmButton: false,
              timer: 1500
            })
        </script>
}
if (TempData["DeleteStatus"]!=null)
{
     <script>
                        Swal.fire({
              position: 'top-end',
              icon: 'success',
              title: 'Your work has been saved',
              showConfirmButton: false,
              timer: 3000
            })
        </script>
}if (TempData["MissionApplicationStatus"]!=null)
{
