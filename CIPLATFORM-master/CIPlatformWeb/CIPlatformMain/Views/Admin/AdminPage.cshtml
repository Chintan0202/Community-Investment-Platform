                        <li><a class="dropdown-item" href="#">Something else here</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="tab-content ms-4" id="v-pills-tabContent">
            <div id="DynamicDiv">

            </div>


        </div>
    </div>

</div>
<script src="~/js/Admin.js">
    </script>

@{
 
    if (TempData["CMSPageRedirect"] != null)
    {
        {
            <script>
                GetCMSList();
        </script>
        }
    }
    else if (TempData["MissionListRedirect"] != null)
    {
        {
            <script>
                GetMissionList();
            </script>
        }
    }
    else if (TempData["BannerManagmentRediect"] != null)
    {

        <script>
            GetBannerList();
        </script>
      
    }
    else
    {
        {
            <script>
                GetUserData();
        </script>
        }
    }
}
